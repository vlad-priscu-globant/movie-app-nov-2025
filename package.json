{
  "name": "movie-app-nov-2025",
  "version": "1.0.0",
  "description": "A modern Vue.js movie discovery app with TMDB API integration, featuring search, pagination, and responsive design",
  "author": "Vlad Priscu",
  "repository": {
    "type": "git",
    "url": "https://github.com/vlad-priscu-globant/movie-app-nov-2025.git"
  },
  "keywords": [
    "vue",
    "typescript",
    "movie",
    "tmdb",
    "tailwind",
    "pinia",
    "vite"
  ],
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.17.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "@vercel/speed-insights": "^1.3.1",
    "pinia": "^3.0.3",
    "tailwindcss": "^4.1.17",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.18.11",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.6.2",
    "typescript": "~5.9.0",
    "vite": "^7.1.11",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vue-tsc": "^3.1.1"
  }
}
