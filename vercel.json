{
  "version": 2,
  "builds": [
    {
      "src": "nuxt.config.ts",
      "use": "@nuxtjs/vercel-builder"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "env": {
    "NUXT_PUBLIC_BASE_URL": "@nuxt_public_base_url"
  },
  "functions": {
    "server/api/**/*.ts": {
      "maxDuration": 10
    }
  }
}